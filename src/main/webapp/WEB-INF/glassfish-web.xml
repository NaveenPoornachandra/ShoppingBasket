<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
    <context-root>/ShoppingBasket</context-root>
    <security-role-mapping>
        <role-name>ADMIN_USER</role-name>
        <group-name>ADMIN_USER</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>REG_USER</role-name>
        <group-name>REG_USER</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>UN_REG_USER</role-name>
        <group-name>UN_REG_USER</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ORDER_PROCESS</role-name>
        <group-name>ORDER_PROCESS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ORDER_DELIVER</role-name>
        <group-name>ORDER_DELIVER</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ORDER_QA</role-name>
        <group-name>ORDER_QA</group-name>
    </security-role-mapping>
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class' java code.</description>
        </property>
    </jsp-config>
</glassfish-web-app>
